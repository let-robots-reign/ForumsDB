{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "member-access": [
      false
    ],
    "import-spacing": false,
    "no-trailing-whitespace": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-angle-bracket-type-assertion": false
  },
  "rulesDirectory": []
}
